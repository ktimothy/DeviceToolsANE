<extension xmlns="http://ns.adobe.com/air/extension/3.1">	<id>com.quaternary.devicetools.DeviceTools</id>	<versionNumber>1.0.0</versionNumber>	<platforms>		<platform name="iPhone-ARM">			<applicationDeployment>				<nativeLibrary>libDeviceTools.a</nativeLibrary>				<initializer>DeviceToolsExtensionInitializer</initializer>			</applicationDeployment>		</platform>		<platform name="Android-ARM">			<applicationDeployment>				<nativeLibrary>libDeviceTools.jar</nativeLibrary>				<initializer>com.quaternary.devicetools.DeviceToolsExtension</initializer>			</applicationDeployment>		</platform>		<platform name="Android-x86">			<applicationDeployment>				<nativeLibrary>libDeviceTools.jar</nativeLibrary>				<initializer>com.quaternary.devicetools.DeviceToolsExtension</initializer>			</applicationDeployment>		</platform>		<platform name="default">			<applicationDeployment/>		</platform>	</platforms></extension>